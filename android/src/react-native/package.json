{
  "name": "@reddimon/react-native-attribution",
  "version": "1.0.1",
  "description": "React Native Attribution SDK",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "src",
    "lib",
    "android",
    "!android/build",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "scripts": {
    "typescript": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "prepare": "bob build",
    "release": "release-it",
    "build": "bob build",
    "test": "jest"
  },
  "keywords": [
    "react-native",
    "android",
    "attribution",
    "tracking"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Davidon4/reddimon-attribution-sdk.git"
  },
  "author": "Davidon4 <dev@reddimon.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Davidon4/reddimon-attribution-sdk/issues"
  },
  "homepage": "https://github.com/Davidon4/reddimon-attribution-sdk#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "devDependencies": {
    "@react-native-community/eslint-config": "^3.0.0",
    "@types/jest": "^29.5.0",
    "@types/react": "^18.3.18",
    "@types/react-native": "^0.72.8",
    "eslint": "^8.19.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.7",
    "react": "18.2.0",
    "react-native": "0.72.0",
    "react-native-builder-bob": "^0.20.4",
    "release-it": "^15.0.0",
    "typescript": "^5.7.3"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/lib/"
    ]
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  }
}
